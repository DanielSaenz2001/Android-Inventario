<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/pProDetail"
    tools:context=".ui.regulaciones.Rege.RegulacionesListDetails">
    <ImageView
        android:id="@+id/imagenProducto"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:background="@color/statusBarColor"
        android:contentDescription="@string/text_post_header"
        android:fitsSystemWindows="true"
        android:scaleType="centerCrop"
        android:transitionName="photo"
        app:layout_collapseMode="parallax"
        app:layout_collapseParallaxMultiplier="0.5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:src="@tools:sample/backgrounds/scenic"/>

    <TextView
        android:id="@+id/textView4"
        android:layout_width="101dp"
        android:layout_height="0dp"
        android:layout_marginStart="12dp"
        android:background="@drawable/rounder0"
        android:gravity="center"
        android:text="Resposable:"
        android:textColor="@color/textDescription"
        app:layout_constraintBottom_toBottomOf="@+id/responsable_regulacion"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView17" />

    <TextView
        android:id="@+id/responsable_regulacion"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="12dp"
        android:background="@drawable/rounder0"
        android:textColor="@color/textColor"
        android:text="responsable_regulacion"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/producto_regulacion" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="101dp"
        android:layout_height="0dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="18dp"
        android:background="@drawable/rounder6"
        android:gravity="center"
        android:text="Fecha:"
        android:textColor="@color/textDescription"
        app:layout_constraintBottom_toBottomOf="@+id/fecha_regulacion"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imagenProducto" />

    <TextView
        android:id="@+id/fecha_regulacion"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="18dp"
        android:layout_marginEnd="12dp"
        android:background="@drawable/rounder5"
        android:text="fecha_regulacion"
        android:textColor="@color/textColor"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/imagenProducto" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="101dp"
        android:layout_height="0dp"
        android:layout_marginStart="12dp"
        android:background="@drawable/rounder0"
        android:gravity="center"
        android:text="Actividad:"
        android:textColor="@color/textDescription"
        app:layout_constraintBottom_toBottomOf="@+id/actividad_regulacion"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <TextView
        android:id="@+id/actividad_regulacion"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="12dp"
        android:background="@drawable/rounder0"
        android:textColor="@color/textColor"
        android:text="actividad_regulacion"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/cantidad_regulacion" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="101dp"
        android:layout_height="0dp"
        android:layout_marginStart="12dp"
        android:background="@drawable/rounder0"
        android:gravity="center"
        android:text="Cantidad:"
        android:textColor="@color/textDescription"
        app:layout_constraintBottom_toBottomOf="@+id/cantidad_regulacion"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/motivo_regulacion"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="12dp"
        android:background="@drawable/rounder3"
        android:textColor="@color/textColor"
        android:text="motivo_regulacion"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textView10"
        app:layout_constraintTop_toBottomOf="@+id/actividad_regulacion" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="101dp"
        android:layout_height="0dp"
        android:layout_marginStart="12dp"
        android:background="@drawable/rounder2"
        android:gravity="center"
        android:text="Motivo:"
        android:textColor="@color/textDescription"
        app:layout_constraintBottom_toBottomOf="@+id/motivo_regulacion"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/cantidad_regulacion"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="12dp"
        android:background="@drawable/rounder0"
        android:textColor="@color/textColor"
        android:text="cantidad_regulacion"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/responsable_regulacion" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="101dp"
        android:layout_height="0dp"
        android:layout_marginStart="12dp"
        android:background="@drawable/rounder0"
        android:gravity="center"
        android:text="Producto:"
        android:textColor="@color/textDescription"
        app:layout_constraintBottom_toTopOf="@+id/responsable_regulacion"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/producto_regulacion"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="12dp"
        android:background="@drawable/rounder0"
        android:textColor="@color/textColor"
        android:text="producto_regulacion"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView17"
        app:layout_constraintTop_toBottomOf="@+id/fecha_regulacion" />
</androidx.constraintlayout.widget.ConstraintLayout>