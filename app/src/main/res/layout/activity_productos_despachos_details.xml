<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/pDesDetail"
    tools:context=".ui.DespachosProductos.details.ProductosDespachosDetails">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/ProductoDespachoDetailsRecyclerView"
        style="@style/DataRecyclerStyle"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="18dp"
        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        tools:listitem="@layout/item_producto_despacho_details" />

    <TextView
        android:id="@+id/conductor_name_details"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="12dp"
        android:background="@drawable/rounder5"
        android:textColor="@color/textColor"
        android:text="conductor_name_details"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.043"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="101dp"
        android:layout_height="0dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/rounder6"
        android:gravity="center"
        android:textColor="@color/textDescription"
        android:text="Conductor:"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="101dp"
        android:layout_height="0dp"
        android:layout_marginStart="12dp"
        android:background="@drawable/rounder0"
        android:gravity="center"
        android:text="Salida:"
        android:textColor="@color/textDescription"
        app:layout_constraintBottom_toBottomOf="@+id/fecha_salida_details"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="101dp"
        android:layout_height="0dp"
        android:layout_marginStart="12dp"
        android:background="@drawable/rounder0"
        android:gravity="center"
        android:textColor="@color/textDescription"
        android:text="Destino:"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="101dp"
        android:layout_height="0dp"
        android:layout_marginStart="12dp"
        android:background="@drawable/rounder0"
        android:gravity="center"
        android:textColor="@color/textDescription"
        android:text="Vehiculo:"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="101dp"
        android:layout_height="0dp"
        android:layout_marginStart="12dp"
        android:background="@drawable/rounder2"
        android:gravity="center"
        android:textColor="@color/textDescription"
        android:text="Responsable:"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <TextView
        android:id="@+id/fecha_salida_details"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="12dp"
        android:background="@drawable/rounder0"
        android:textColor="@color/textColor"
        android:text="fecha_salida_details"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/conductor_name_details" />

    <TextView
        android:id="@+id/ciudad_destino_details"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="12dp"
        android:background="@drawable/rounder0"
        android:textColor="@color/textColor"
        android:text="ciudad_destino_details"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/fecha_salida_details" />

    <TextView
        android:id="@+id/vehiculo_details"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="12dp"
        android:background="@drawable/rounder0"
        android:textColor="@color/textColor"
        android:text="vehiculo_details"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.084"
        app:layout_constraintStart_toEndOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/ciudad_destino_details" />

    <TextView
        android:id="@+id/responsable_details"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="12dp"
        android:background="@drawable/rounder3"
        android:textColor="@color/textColor"
        android:text="responsable_details"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textView10"
        app:layout_constraintTop_toBottomOf="@+id/vehiculo_details" />

</androidx.constraintlayout.widget.ConstraintLayout>